import React, { useState }  from "react";
import {
    StyleSheet,
    Text,
    Button,
    View,
    Image,
    TextInput,
    TouchableOpacity,
    Picker,
} from "react-native";

import { NavigationContainer } from '@react-navigation/native';
import { createStackNavigator } from '@react-navigation/stack';

import api from "../dsm/services/api"

function HomeScreen({ navigation }) {
    const entrar = () => {
        navigation.reset({
            index: 0,
            routes: [{name:"principal"}]
        })
    }
    return (
        <View style={styles.container}>
            <Image
                style={styles.logo}
                source={{
                    uri: "data:image/jpeg;base64,",
                }} />
            <TextInput style={styles.input} placeholder="digite seu email" />
            <TextInput
                style={styles.input}
                secureTextEntry={true}
                placeholder="digite sua senha"
            />
            <TouchableOpacity style={styles.botao}
                onPress={() => entrar()}
            >
                <Text style={styles.botaoLogin}> Login </Text>{" "}
            </TouchableOpacity>{" "}
            <TouchableOpacity style={styles.botaocadastro}
                onPress={() => navigation.navigate('cadastro')}
            >
                <Text style={styles.botaoLogin}> Cadastro </Text>{" "}
            </TouchableOpacity>{" "}
        </View>
    );
}

function principalScreen({ navigation }) {

    const [selectedValue, setSelectedValue] = useState("");

    constructor(props){
        super(props);
        this.state = {
            estados: []
        };
    };
    const getEstado = async () => {
        const response = await api.get(`${selectedValue}`);
        estados = response.data
        console.log(response)
    };



    return (
        <View style={styles.container}>
            <Picker
                selectedValue={selectedValue}
                style={{ height: 50, width: 150 }}
                onValueChange={(itemValue, itemIndex) => setSelectedValue(itemValue)}
                
            >
                <Picker.Item label="Java" value="sp" />
                <Picker.Item label="JavaScript" value="ba" />
            </Picker>
            <TouchableOpacity style={styles.botao}
                onPress={() => getEstado}
            >
                <Text style={styles.botaoLogin}> Login </Text>{" "}
            </TouchableOpacity>
            <Text>
                {selectedValue}
            </Text>
            <view>
                {<Text>
                    {this.estados.state}
                </Text>}
            </view>
        </View>

    );
}


function cadastroScreen({ navigation }) {
    return (
        <View style={{ flex: 1, alignItems: 'center', justifyContent: 'center' }}>
            <TouchableOpacity style={styles.botao}
                onPress={() => navigation.navigate('Profile')}
            >
                <Text style={styles.botaoLogin}> Login </Text>{" "}
            </TouchableOpacity>{" "}
        </View>
    );
}

const Stack = createStackNavigator();

function MyStack() {
    return (
        <Stack.Navigator>
            <Stack.Screen name="Home" component={HomeScreen} />
            <Stack.Screen name="principal" component={principalScreen} />
            <Stack.Screen name="cadastro" component={cadastroScreen} />
        </Stack.Navigator>
    );
}

export default function App() {
    return (
        <NavigationContainer>
            <MyStack />
        </NavigationContainer>
    );
}
const styles = StyleSheet.create({
    container: {
        flex: 1,
        justifyContent: "center",
        alignItems: "center",
        backgroundColor: "#2c3e50",
    },
    logo: {
        resizeMode: "contain",
        width: 150,
        height: 150,
        marginBottom: 70,
        borderRadius: 150,
    },
    input: {
        marginTop: 10,
        padding: 10,
        width: 300,
        backgroundColor: "#fff",
        fontSize: 16,
        fontWeight: "bold",
        borderRadius: 3,
    },
    botao: {
        width: 300,
        height: 42,
        backgroundColor: "#3498db",
        marginTop: 10,
        borderRadius: 4,
        alignItems: "center",
        justifyContent: "center",
    },
    botaocadastro: {
        width: 300,
        height: 42,
        marginTop: 10,
        borderRadius: 4,
        alignItems: "center",
        justifyContent: "center",
    },
    botaoLogin: {
        fontSize: 16,
        fontWeight: "bold",
        color: "#fff",
    },
});
